<Window x:Class="PuppyQuiz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PuppyQuiz"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="30" Margin="244,31,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="195"/>
        <Label Content="What is your first name?" HorizontalAlignment="Left" Height="34" Margin="52,31,0,0" VerticalAlignment="Top" Width="171"/>
        <Label Content="What is your favorite color?" HorizontalAlignment="Left" Height="31" Margin="52,69,0,0" VerticalAlignment="Top" Width="160"/>
        <RadioButton Content="Blue" HorizontalAlignment="Left" Height="18" Margin="315,73,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="Red" HorizontalAlignment="Left" Height="18" Margin="423,73,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="Green" HorizontalAlignment="Left" Height="18" Margin="522,73,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="Purple" HorizontalAlignment="Left" Height="18" Margin="616,73,0,0" VerticalAlignment="Top" Width="94"/>
        <Label Content="What is your favorite animal?" HorizontalAlignment="Left" Height="32" Margin="52,109,0,0" VerticalAlignment="Top" Width="171"/>
        <RadioButton Content="Giraffe" HorizontalAlignment="Left" Height="18" Margin="315,115,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="Elephant" HorizontalAlignment="Left" Height="18" Margin="423,115,0,0" VerticalAlignment="Top" Width="94" RenderTransformOrigin="-0.032,0.644"/>
        <RadioButton Content="Lion" HorizontalAlignment="Left" Height="18" Margin="522,115,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="Penguin" HorizontalAlignment="Left" Height="18" Margin="616,115,0,0" VerticalAlignment="Top" Width="94"/>
        <Label Content="What is your favorite season?" HorizontalAlignment="Left" Height="30" Margin="52,150,0,0" VerticalAlignment="Top" Width="171"/>
        <RadioButton Content="Winter" HorizontalAlignment="Left" Height="18" Margin="315,154,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="Spring" HorizontalAlignment="Left" Height="18" Margin="423,154,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="Summer" HorizontalAlignment="Left" Height="18" Margin="522,154,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="Fall" HorizontalAlignment="Left" Height="18" Margin="616,154,0,0" VerticalAlignment="Top" Width="94"/>
        <Label Content="What is your favorite food?" HorizontalAlignment="Left" Height="30" Margin="52,223,0,0" VerticalAlignment="Top" Width="160"/>
        <Label Content="1." HorizontalAlignment="Left" Height="34" Margin="10,31,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="2." HorizontalAlignment="Left" Height="34" Margin="10,69,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="3." HorizontalAlignment="Left" Height="34" Margin="10,109,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="4." HorizontalAlignment="Left" Height="34" Margin="10,148,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="5." HorizontalAlignment="Left" Height="34" Margin="10,189,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="6." HorizontalAlignment="Left" Height="34" Margin="10,223,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="7." HorizontalAlignment="Left" Height="34" Margin="10,262,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="8." HorizontalAlignment="Left" Height="34" Margin="10,296,0,0" VerticalAlignment="Top" Width="24" RenderTransformOrigin="0.492,0.688"/>
        <Label Content="9." HorizontalAlignment="Left" Height="34" Margin="10,330,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="10." HorizontalAlignment="Left" Height="34" Margin="10,364,0,0" VerticalAlignment="Top" Width="24"/>
        <Label Content="What is your favorite ice cream flavor?" HorizontalAlignment="Left" Height="33" Margin="52,262,0,0" VerticalAlignment="Top" Width="222"/>
        <Label Content="What age range are you in?" HorizontalAlignment="Left" Height="34" Margin="52,189,0,0" VerticalAlignment="Top" Width="160"/>
        <Label Content="What do you like to do on a rainy day?" HorizontalAlignment="Left" Height="30" Margin="52,300,0,0" VerticalAlignment="Top" Width="222"/>
        <Label Content="What is your eye color?" HorizontalAlignment="Left" Height="29" Margin="52,335,0,0" VerticalAlignment="Top" Width="222"/>
        <Label Content="Which one is better?" HorizontalAlignment="Left" Height="34" Margin="52,364,0,0" VerticalAlignment="Top" Width="141"/>
        <RadioButton Content="0 - 18" HorizontalAlignment="Left" Height="18" Margin="315,195,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="19 - 30" HorizontalAlignment="Left" Height="18" Margin="423,195,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="31 - 60" HorizontalAlignment="Left" Height="18" Margin="522,195,0,0" VerticalAlignment="Top" Width="94"/>
        <RadioButton Content="61 - Above" HorizontalAlignment="Left" Height="18" Margin="616,195,0,0" VerticalAlignment="Top" Width="94"/>

    </Grid>
</Window>
